{
    "name": "coba-storage",
    "version": "1.0.2",
    "main": "di.js",
    "bin": {
        "coba": "bin.js"
    },
    "description": "An open sourced high-speed storage. Based on Roaring Bitmaps technology.",
    "author": "me@ejz.me",
    "license": "MIT",
    "scripts": {
        "prewebpack": "rm -rf dist",
        "webpack": "NODE_ENV=production webpack --config webpack.js --progress",
        "lint": "eslint --fix --quiet src/*.js",
        "pretest": "npm run lint",
        "test": "jest --runInBand",
        "prebuild": "npm run test",
        "build": "npm run webpack && node etc/package-json",
        "prepublish": "npm run build",
        "publish": "cd dist && npm publish"
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.3.6",
        "@grpc/proto-loader": "^0.6.4",
        "ejz-grpc": "^1.0.10",
        "ejz-utils": "^1.5.8",
        "fast-crc32c": "^2.0.0",
        "roaring": "^1.0.6",
        "tcp-port-used": "^1.0.2"
    },
    "devDependencies": {
        "copy-webpack-plugin": "^9.0.1",
        "eslint": "^8.0.0",
        "jest": "^27.0.6",
        "webpack": "^5.52.0",
        "webpack-cli": "^4.8.0",
        "webpack-node-externals": "^3.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/ejz/coba.git"
    }
}
